{
  "settings": {
    "index.refresh_interval": "5s",
    "index": {
      "query": {
        "default_field": "description"
      }
    }
  },
  "mappings": {
    "properties": {
      "user": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 256
          }
        }
      },
      "category": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 256
          }
        }
      },
      "query": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 256
          }
        },
        "analyzer": "english"
      },
      "click_time": {
        "type": "date",
        "format": "yyyy-MM-dd[ ]['T']HH:mm:ss[.][SSS][SS][S][X]"
      },
      "query_time": {
        "type": "date",
        "format": "yyyy-MM-dd[ ]['T']HH:mm:ss[.][SSS][SS][S][X]"
      }
    }
  }
}